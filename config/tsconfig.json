{
 "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowjs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSynthicDefaultImports": true,
    "strict": false,
    "forceConsistentCastingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
 },

 "include": ["src"]
}

    